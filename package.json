{
  "name": "leet-cph",
  "displayName": "CPH LeetCode",
  "publisher": "S0r4-0",
  "description": "Streamlines problem-solving for LeetCode by automating fetching test cases, compiling code and testing against expected output",
  "version": "0.0.1",
  "license": "MIT",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/S0r4-0/leet-cph.git"
  },
  "contributes": {
    "commands": [
      {
        "command": "cph-leetcode.getTestCases",
        "title": "CPH: Fetch Test Cases"
      },
      {
        "command": "cph-leetcode.runTestCases",
        "title": "CPH: Run Test Cases"
      }
    ],
    "configuration": {
      "title": "Leetcode Helper",
      "properties": {
        "cph.language.cpp.compile": {
          "type": "string",
          "default": "g++",
          "description": "The command to compile C++ files"
        },
        "cph.language.cpp.OutputArg": {
          "type": "string",
          "default": "-o",
          "description": "C++ compiler argument to specify output file"
        },
        "cph.language.python.run": {
          "type": "string",
          "default": "python",
          "description": "The command to run Python files"
        }
      }
    },
    "keybindings": [
      {
        "command": "cph-leetcode.getTestCases",
        "key": "ctrl+alt+a"
      },
      {
        "command": "cph-leetcode.runTestCases",
        "key": "ctrl+alt+b"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "deploy": "vsce publish"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/puppeteer": "^7.0.4",
    "@types/vscode": "^1.96.0",
    "@typescript-eslint/eslint-plugin": "^8.17.0",
    "@typescript-eslint/parser": "^8.17.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.7.2",
    "webpack": "^5.95.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "puppeteer": "^23.11.1"
  }
}
